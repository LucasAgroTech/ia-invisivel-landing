{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.vercel/output",
  "framework": null,
  "functions": {
    "api/leads.py": {
      "runtime": "@vercel/python"
    },
    "api/remaining-spots.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(q-data/.*)",
      "destination": "/$1"
    },
    {
      "source": "/(build/.*)",
      "destination": "/$1"
    },
    {
      "source": "/(assets/.*)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "cache-control", 
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/q-data/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 
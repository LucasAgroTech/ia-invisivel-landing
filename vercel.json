{
  "buildCommand": "cd frontend && npm ci && npm run build",
  "outputDirectory": "frontend/dist", 
  "installCommand": "cd frontend && npm ci",
  "framework": null,
  "functions": {
    "api/leads.py": {
      "runtime": "@vercel/python@4.3.0"
    },
    "api/remaining-spots.py": {
      "runtime": "@vercel/python@4.3.0"
    }
  },
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "regions": ["iad1"]
} 